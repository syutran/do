<%= render :partial => "navbar" %>
<ul class="thumbnails">
  <% @user.each do |user| %>
    <li class="span3" id="join_me">
    <%= user_face(user) %>
    <div class="caption">
      <h5><%= user.name %></h5>
      <%= link_to "info", user_info_url(user_id: user.id ), class: 'btn' if user.master.map{|m| m.friend_id}.include?current_user.id %> 
        <%= button_to "add_friend", circles_join_me_path(user_id: user.id),:class => "btn btn-success", :remote => true unless user.master.map{|m| m.friend_id}.include?current_user.id %>
    </div>
    </li>
  <% end %>
</ul>
