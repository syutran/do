<% @user.each do |user| %>
  <%= image_tag avatar_url( user) %>
  <%= user.name %>
  <%= link_to "info", user_info_url(user_id: user.id ), class: 'btn' %> 
  <%= button_to "add_friend", circles_join_me_path(user_id: user.id), remote: true %>
<% end %>
<div id="join_me"><%= "#{root_url}images/guest.png" %></div>
