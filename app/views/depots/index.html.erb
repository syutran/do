<%= render 'navbar' %>
<h3>题库列表</h3>


<% @depots.each do |depot| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading"><%= depot.category.title %></h4>
      <div class="media">
        <%= depot.title %>
      </div>
    </div>
  </div>
    <%= link_to '查看', depot %>
    <%= link_to '编辑', edit_depot_path(depot) %>
    <%= link_to '删除', depot, method: :delete, data: { confirm: 'Are you sure?' } %>
<% end %>

<br />

<%= link_to 'New Depot', new_depot_path %>
