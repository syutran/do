<!DOCTYPE html>
<html>
  <head>
    <title>Do</title>
    <%= stylesheet_link_tag "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div class="container">
      <div class="modal-head">
      <div class="row">
        <%= render :partial => "/header" %>
      </div>
    </div>
      <div class="navbar">
        <div class="navbar-inner">
          <div class="container">
            <%= yield :navbar %>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="span8">
          <%= yield %>
        </div>
        <div class="span4">
          <%= render :partial => "sidebar" %>
        </div>
      </div>
      <footer class="modal-footer">
      <p>&copy; Company 2011 by <a href='http://syutran.com/' target='_blank'>Syutran blog</a></p>
      </footer>
    </div>
  </body>
</html>
